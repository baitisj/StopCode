<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
	
		.important {
			font-weight: bold;
		}
	
	</ui:style>
	
	<g:DockLayoutPanel unit='PCT' styleName="gwt-DockPanel">
		<g:north size='1'>
			<g:FlowPanel ui:field="N"/>
		</g:north>
		<g:west size='15'>
			<g:ScrollPanel ui:field="W" styleName="gwt-navContainer"/>
		</g:west>
		<g:center size='85'>
			<g:ScrollPanel ui:field="M" styleName="gwt-mainContainer"/>
		</g:center>
		<g:south size='10'>
			<g:FlowPanel ui:field="S" styleName="gwt-buttonContainer">

              <g:Button styleName="gwt-button" text="Prev" ui:field="prev"></g:Button> 

              <g:Button styleName="gwt-button" text="Next" ui:field="next"></g:Button> 

			</g:FlowPanel>
		</g:south>
	</g:DockLayoutPanel>
	
</ui:UiBinder> 