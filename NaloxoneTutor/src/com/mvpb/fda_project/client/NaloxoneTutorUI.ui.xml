<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
	
		.important {
			font-weight: bold;
		}
	
	</ui:style>
	
	<g:DockLayoutPanel unit='PCT' styleName="gwt-DockPanel">
		<g:north size='1'>
			<g:FlowPanel ui:field="N"/>
		</g:north>
		<g:west size='10'>
			<g:ScrollPanel ui:field="W" styleName="gwt-navContainer"/>
		</g:west>
		<g:center size='90'>
			<g:FlowPanel ui:field="M" styleName="gwt-mainContainer"/>
		</g:center>
		<g:south size='1'>
			<g:FlowPanel ui:field="S"/>
		</g:south>
	</g:DockLayoutPanel>
	
</ui:UiBinder> 